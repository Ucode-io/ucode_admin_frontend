{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest", // 2015
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["off", "double"]
  }
}
